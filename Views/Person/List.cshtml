@using PTPM.Models
<h2>Danh sách sinh viên</h2>
<hr>
<table class="table border">
    <tr>
        <th>
            <label>Mã sinh viên</label>
        </th>
        <th>
            <label>Tên sinh viên</label>
        </th>
        <th>
            <label>Tuổi</label>
        </th>
    </tr>
    @foreach (var item in ViewData["Person"] as List<Person>)
        {
            <tr>
                <td>@item.PersonID</td>
                <td>@item.Personstring</td>
                <td>@item.PersonAge</td>
            </tr>
        }
</table>